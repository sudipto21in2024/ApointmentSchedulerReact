      "easeInOut": "cubic-bezier(0.4, 0, 0.2, 1)",
      "bounce": "cubic-bezier(0.68, -0.55, 0.265, 1.55)"
    },

    "// Animation durations": "",
    "duration": {
      "fast": "150ms",
      "normal": "200ms",
      "slow": "300ms",
      "slower": "500ms"
    },

    "// Common animation presets": "",
    "presets": {
      "fadeIn": {
        "duration": "200ms",
        "easing": "ease-out",
        "fillMode": "forwards"
      },
      "fadeOut": {
        "duration": "200ms",
        "easing": "ease-in",
        "fillMode": "forwards"
      },
      "slideIn": {
